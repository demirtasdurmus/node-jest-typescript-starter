{
    "name": "node-jest-typescript-starter",
    "version": "2.1.0",
    "scripts": {
        "commit": "commit",
        "clean": "rimraf dist",
        "dev": "nodemon --watch 'src/**' --ext 'ts,json' --ignore 'src/**/*.test.ts' --exec 'ts-node --files -H -T src/index.ts'",
        "build": "rimraf dist && tsc --pretty -p tsconfig.build.json",
        "start": "node dist/index.js",
        "format:write": "prettier --write \"**/*.ts\" --cache",
        "format:check": "prettier --check \"**/*.ts\" --cache",
        "lint": "eslint \"**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watchAll --no-cache  --detectOpenHandles",
        "test:e2e": "jest --config ./tests/jest-e2e.config.ts",
        "test:cov": "jest --coverage",
        "danger:ci": "danger ci",
        "prepare": "husky install"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@commitlint/cli": "^17.6.5",
        "@commitlint/config-conventional": "^17.6.5",
        "@commitlint/prompt-cli": "^17.6.5",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@types/jest": "^29.5.1",
        "@types/node": "^18.16.3",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.59.2",
        "@typescript-eslint/parser": "^5.59.2",
        "danger": "^11.2.8",
        "eslint": "^8.39.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-prettier": "^5.1.2",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "lint-staged": "^13.2.2",
        "nodemon": "^3.0.2",
        "prettier": "^3.1.1",
        "prettier-plugin-organize-imports": "^3.2.4",
        "rimraf": "^5.0.1",
        "semantic-release": "^21.0.7",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    }
}
